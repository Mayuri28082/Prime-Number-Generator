{
    "panels": [
        {
            "type": "stat",
            "title": "Total HTTP Requests",
            "targets": [
                {
                    "expr": "http_requests_total"
                }
            ]
        },
        {
            "type": "graph",
            "title": "Celery Task Count",
            "targets": [
                {
                    "expr": "celery_tasks_total"
                }
            ]
        },
        {
            "type": "graph",
            "title": "Celery Task Duration",
            "targets": [
                {
                    "expr": "celery_task_duration_seconds_sum / celery_task_duration_seconds_count or vector(0)"
                }
            ]
        },
        {
            "type": "graph",
            "title": "RabbitMQ Messages Ready",
            "targets": [
                {
                    "expr": "rabbitmq_queue_messages_ready{queue=\"celery\"}"
                }
            ]
        },
        {
            "type": "graph",
            "title": "CPU Usage",
            "targets": [
                {
                    "expr": "rate(node_cpu_seconds_total[5m])"
                }
            ]
        },
        {
            "type": "graph",
            "title": "Memory Available",
            "targets": [
                {
                    "expr": "node_memory_MemAvailable_bytes"
                }
            ]
        }
    ]
}